      <Placemark>
        <name>Myrna Shoe Company</name>
        <description>Langdon Mill #2</description>
        <styleUrl>#icon-1899-0288D1</styleUrl>
        <Point>
          <coordinates>
            -71.4675665,42.9978521,0
